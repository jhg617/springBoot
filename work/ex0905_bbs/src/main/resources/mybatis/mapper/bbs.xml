<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sist.ex0905_bbs.mapper.BbsMapper">
    <select id="list" resultType="com.sist.ex0905_bbs.vo.BbsVO">
        select * from bbs_t
    </select>

    <select id="totalCount" resultType="com.sist.ex0905_bbs.vo.BbsVO" parameterType="String">
        select count(*) from bbs_t
        where status 0 AND bname = #{bname}
    </select>
</mapper>